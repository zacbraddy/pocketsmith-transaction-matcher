# PocketSmith Transaction Matcher Environment Variables
# Copy this file to .env and update the values

# PocketSmith API Configuration
POCKETSMITH_API_KEY=your_pocketsmith_api_key_here
POCKETSMITH_BASE_URL=https://api.pocketsmith.com/v2

# Unirate API Configuration
UNIRATE_API_KEY=your_unirate_api_key_here

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info

# File Processing
INPUT_CSV_PATH=./data/input
OUTPUT_PATH=./data/output
BACKUP_PATH=./data/backup

# Matching Configuration
MATCHING_THRESHOLD=0.8
AUTO_APPROVE_MATCHES=false
BATCH_SIZE=100

# Database (if needed)
# DATABASE_URL=sqlite:./data/transactions.db

# Optional: Rate limiting for API calls
API_RATE_LIMIT_MS=1000
