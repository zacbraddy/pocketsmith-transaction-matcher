# PocketSmith Transaction Matcher Environment Configuration
# Copy this file to .env and update the values for your environment

# Required: PocketSmith API Configuration
POCKETSMITH_API_KEY=your_actual_api_key_here
POCKETSMITH_BASE_URL=https://api.pocketsmith.com/v2

# Required: UniRate API Configuration
UNIRATE_API_KEY=your_unirate_api_key_here

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info

# File Processing
INPUT_CSV_PATH=./data/input

# Tolerance Configuration
DAYS_TOLERANCE=2
DATE_RANGE_BUFFER_DAYS=5
AMOUNT_TOLERANCE_EXACT=0.001
AMOUNT_TOLERANCE_FOREIGN_PERCENT=0.1

# API Rate Limiting
API_RATE_LIMIT_MS=1000
